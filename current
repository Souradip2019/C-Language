#include<stdio.h>
int main ()
{
    int u;
    float charge;
    printf ("enter u: ");
    scanf ("%d",&u);
    if  (u<=25)
        charge=25*4.89;
    else if (u>=26 && u<=60)
        charge=25*4.89+5.40*(u-25);
    else if (u>=61 && u<=100)
        charge=25*4.89+5.40*(u-25)+6.41*(u-25-35);
     else if (u>=101 && u<=150)
        charge=25*4.89+5.40*(u-25)+6.41*(u-25-35)+7.16*(u-25-35-40);
        else if (u>=151 && u<=200)
        charge=25*4.89+5.40*(u-25)+6.41*(u-25-35)+7.16*(u-25-35-40)+7.33*(u-25-35-40-50);
        else if (u>=201 && u<=300)
        charge=25*4.89+5.40*(u-25)+6.41*(u-25-35)+7.16*(u-25-35-40)+7.33*(u-25-35-40-50)+7.33*(u-25-35-40-50-50);
        else if (u>=301 && u<=600)
        charge=25*4.89+5.40*(u-25)+6.41*(u-25-35)+7.16*(u-25-35-40)+7.33*(u-25-35-40-50)+7.33*(u-25-35-40-50-50)+8.92*(u-25-35-40-50-50-100);
        else if (u>600)
        charge=25*4.89+5.40*(u-25)+6.41*(u-25-35)+7.16*(u-25-35-40)+7.33*(u-25-35-40-50)+7.33*(u-25-35-40-50-50)+8.92*(u-25-35-40-50-50-100);
        printf ("charge is %f",charge);

}
