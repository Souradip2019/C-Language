//    Swap the value of two Variables(without using 3rd variable).
#include<stdio.h>
int main ()
{
    int a,b;
    printf ("\n..........Swap the value of two Variables......../n/n/n/n");
    printf ("\nEnter two values :");
    scanf ("%d%d",&a,&b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf ("\nAfter swapping a=%d b=%d",a,b);
    return 0;
}
