//..................(1+3+5+7+9+11+13+.............)
#include <stdio.h>
int main ()
{
    int range,i,sum=0;
    printf ("............1+3+5+7+9+11+13+...........");
    printf ("\nEnter the range :: ");
    scanf ("%d",&range);
    printf ("\nSeries is:: ");
    for (i=0;i<=range;i++)
    {
        if (i%2!=0)
        {
        printf ("%d ",+i);
        sum=sum+i;
        }
    }
    printf ("\n\nSUM=%d",sum);
    return 0;
}
