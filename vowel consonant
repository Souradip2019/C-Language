#include <stdio.h>
int main ()
{

char c;
int no;
    printf ("Enter the character:  ");
    scanf ("%c",&c);
    if ((c>=65&&c<=90) || (c>=97&&c<=122))
        switch (c)
        {
        case 'a' :
        case 'e' :
        case 'i' :
        case 'o' :
        case 'u' :
        case 'A' :
        case 'E' :
        case 'I' :
        case 'O' :
        case 'U' :
        printf ("VOWEL");
        break;
        default : printf ("CONSONANT");
        }
return 0;
}
